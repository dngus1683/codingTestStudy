// DFS와 BFS
// 그래프를 DFS로 탐색한 결과와 BFS로 탐색한 결과를 출력하는 프로그램을 작성하시오. 단, 방문할 수 있는 정점이 여러 개인 경우에는 정점 번호가 작은 것을 먼저 방문하고, 더 이상 방문할 수 있는 점이 없는 경우 종료한다. 정점 번호는 1번부터 N번까지이다.


// 내 답안1
#include <iostream>
#include <queue>
#include <algorithm>
#include <vector>

#define MAX_N 1001
#define MAX_M 10001

using namespace std;

int N, M, V;
int graph[MAX_N][MAX_N];
int ADFS[MAX_N];
int ABFS[MAX_N];

void DFS(int start)
{
    for(int i=1; i<=N; i++)
    {
        if(graph[start][i] == 1)
        {
            DFS(graph[start][i]);
        }
    }

}

void BFS(int start)
{

}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    fill(&graph[0][0], &graph[MAX_N-1][MAX_N], 0);

    cin >> N >> M >> V;
    for(int i=0; i<M; i++)
    {
        int a, b;
        cin >> a >> b;
        graph[a][b] = 1;
        graph[b][a] = 1;
    }


}
